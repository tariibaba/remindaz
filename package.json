{
  "name": "reminders",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "author": {
    "name": "Ayibatari Ibaba",
    "email": "tariibabadev@gmail.com",
    "url": "https://github.com/tariibaba"
  },
  "build": {
    "appId": "com.tariibaba.reminders",
    "productName": "Reminders",
    "artifactName": "Reminders-Installer-${version}-${os}-${arch}.${ext}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.73",
    "@mui/material": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/uuid": "^8.3.4",
    "date-fns": "^2.28.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-windows-badge": "^1.1.0",
    "mobx": "^6.4.2",
    "mobx-react": "^7.3.0",
    "node-notifier": "^10.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-windows": "^1.0.0-beta.7",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "rstart": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start": "electron --remote-debugging-port=9223 .",
    "dev": "nodemon --exec \"yarn start\"",
    "postinstall": "electron-builder install-app-deps",
    "preepack": "yarn build",
    "epack": "electron-builder --dir",
    "predist": "yarn build",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "nodemon": "^2.0.15",
    "react-app-rewired": "^2.2.1",
    "typescript": "^4.6.3"
  }
}
